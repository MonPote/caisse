<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="model.StoreEntity">
        <table name="store" schema="public" catalog="entrepot"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" length="64"/>
            </basic>
            <basic name="adress">
                <column name="adress"/>
            </basic>
            <basic name="zipcode">
                <column name="zipcode" length="16"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
