<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Entities.CommandEntity" table="Command" schema="Back Office" catalog="postgres">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="ref">
            <column name="Ref" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="dateEnvoi">
            <column name="Date_envoi" sql-type="date"/>
        </property>
        <property name="dateReception">
            <column name="Date_reception" sql-type="date" not-null="true"/>
        </property>
        <property name="idClient">
            <column name="id_client" sql-type="integer" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>