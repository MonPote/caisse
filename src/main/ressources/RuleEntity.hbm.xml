<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Entities.RuleEntity" table="Rule" schema="Back Office" catalog="postgres">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="montant">
            <column name="Montant" sql-type="integer"/>
        </property>
        <property name="resultat">
            <column name="Resultat" sql-type="integer" not-null="true"/>
        </property>
        <property name="unit">
            <column name="Unit" sql-type="&quot;unit_type&quot;" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>